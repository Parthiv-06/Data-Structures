def longest_substring_without_repeating_characters(s):
    mpp={}
    l,r,max_len=0,0,0
    for r in range(len(s)):
        mpp[s[r]]=mpp.get(s[r],0)+1
        if mpp[s[r]]==2:
            mpp[s[r]]=mpp.get(s[r],0)-1
            l+=1
        max_len=max(max_len,r-l+1)
    return max_len
s="abcdabcbb"
print(longest_substring_without_repeating_characters(s))